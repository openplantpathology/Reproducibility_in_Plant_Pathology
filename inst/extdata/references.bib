@Article{delesma-morales_pathosystem_2019,
  author   = {Delesma-Morales, Lucio and Lozoya-Saldaña, Héctor and Rodríguez-Pérez, J. Enrique and Díaz-de la Cruz, Jaime B.},
  journal  = {Revista Mexicana de Fitopatología, Mexican Journal of Phytopathology},
  title    = {Pathosystem {Solanum} lycopersicum-{Phytophthora} infestans in {Chapingo}, {Mexico}. {Expected}, observed and simulated},
  year     = {2019},
  issn     = {2007-8080},
  month    = dec,
  number   = {1},
  volume   = {38},
  abstract = {{\textless}em{\textgreater}Phytophthora infestans{\textless}/em{\textgreater} es afectado por factores bióticos y abióticos que pueden hacer variar su patogenicidad. En tierras altas de México, la variabilidad genética de {\textless}em{\textgreater}P. infestans{\textless}/em{\textgreater}, bajas temperaturas, alta humedad relativa y luz favorecen su expresión e influyen en la eficacia de la infección. Este trabajo analizó el progreso de infección de {\textless}em{\textgreater}P. infestans{\textless}/em{\textgreater} respecto a lo esperado, observado y simulado durante dos ciclos de campo en Chapingo, México, con el objetivo de documentar y cuantificar el progreso de la infección foliar por {\textless}em{\textgreater}P. infestans{\textless}/em{\textgreater} en diferentes líneas de jitomate con diferentes niveles de resistencia. Así como validar un modelo predictivo. Con el registro de 10 años de datos meteorológicos, se predijeron cinco situaciones con cuatro a diez ciclos de infección de 8 a 13 h cada uno. En comparación con las predicciones, el efecto observado de área bajo la curva de progreso de la enfermedad (AUDPC) y sus componentes (RAUDPC y RaRAUDPC) fueron significativos, con bajo coeficiente de variación. Se concluye que entre los escenarios esperados, observados y simulados existe una estrecha relación para predecir epidemias de {\textless}em{\textgreater}P. infestans{\textless}/em{\textgreater}. Esto confirma que LATEBLIGHT es útil para simular y predecir la epidemia de tizón tardío, en condiciones atmosféricas de Chapingo.},
  doi      = {10.18781/R.MEX.FIT.1910-5},
  file     = {Full Text:/Users/adamsparks/Zotero/storage/MLWJV6MP/delesma-morales_pathosystem_2019 - Pathosystem Solanum Lycopersicum Phytophthora Infestans in Chapingo, Mexico. Expected, Observed and Simulated.pdf:application/pdf},
  url      = {http://rmf.smf.org.mx/ojs/index.php/RMF/article/view/193},
  urldate  = {2022-02-10},
}

