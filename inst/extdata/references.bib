@article{pedram_additional_2017,
	title = {Additional data on {Iranian} trichodorids ({Triplonchida}: {Trichodoridae}) and first record of a rare species, {Trichodorus} variabilis {Roca}, 1998},
	volume = {19},
	issn = {1388-5545, 1568-5411},
