@article{lopez-bautista_morphological_2019,
	title = {Morphological and molecular characterization of {Fusarium} spp. associated to the regional occurrence of wilt and dry bud rot in {Agave} tequilana},
	volume = {38},
	issn = {2007-8080},
	url = {http://rmf.smf.org.mx/ojs/index.php/RMF/article/view/201},
	doi = {10.18781/R.MEX.FIT.1911-4},
	abstract = {{\textless}p{\textgreater}La marchitez y pudrición seca del cogollo del agave ({\textless}em{\textgreater}Agave tequilana{\textless}/em{\textgreater} var. azul) son enfermedades de alto impacto económico para este cultivo. En este trabajo se planteó determinar la implicación de {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} spp. en ambas enfermedades bajo un enfoque regional. Se colectaron muestras de raíz y suelo en 40 plantaciones comerciales ubicadas en 13 municipios de Los Altos Jalisco, importante región de cultivo de agave azul en México. De cada plantación de colecta se estimó carga de inóculo mediante un índice de {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} obtenido de unidades formadoras de colonias ({\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} vs hongos totales) y se analizó su relación con pH y materia orgánica. Se obtuvieron 109 aislados caracterizados morfológicamente como {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} spp. de los cuales se seleccionaron 25 para identificación molecular con ITS y EF-1a. La selección consideró sintomatología, caracteres macro y microscópicos y prevalencia de tipologías de colonia observadas {\textless}em{\textgreater}in vitro{\textless}/em{\textgreater} en medios Komada, Sabouraud, SNA y CLA. Los caracteres culturales y morfológicos evaluados fueron: coloración micelial, tamaño, forma y septación de macro y microconidios, y longitud y número de fiálides. Se asociaron cinco especies con marchitez y/o pudrición seca ubicadas en tres complejos filogenéticos: {\textless}em{\textgreater}F. oxysporum{\textless}/em{\textgreater} del complejo de especies {\textless}em{\textgreater}Fusarium oxysporum{\textless}/em{\textgreater} (FOSC) con 56\% (46.2\% suelo y 66.7\% raíz) de representatividad regional; {\textless}em{\textgreater}F. solani, F. falciforme{\textless}/em{\textgreater} y {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} sp. del complejo {\textless}em{\textgreater}Fusarium solani{\textless}/em{\textgreater} (FSSC) (40\%); y {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} sp. del complejo {\textless}em{\textgreater}Fusarium fujikuroi{\textless}/em{\textgreater} (FFSC) (4\%). MO y pH tuvieron correlación inversamente proporcional con Índice de {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} ({\textless}em{\textgreater}r2{\textless}/em{\textgreater} = 0.68-0.70). Se postula que la marchitez y pudrición seca del cogollo de agave azul constituyen un síndrome en el cual se asocian y especializan parasíticamente diversas especies de {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater}. Se encontró un aislado de los tres complejos de {\textless}em{\textgreater}Fusarium{\textless}/em{\textgreater} asociados específicamente a cada tipo de síntoma y la combinación de ellos. La mayoría se asociaron a marchitez con predominancia de{\textless}em{\textgreater} F. oxysporum. {\textless}/em{\textgreater}  {\textless}/p{\textgreater}{\textless}p{\textgreater} {\textless}/p{\textgreater}},
	number = {1},
	urldate = {2022-02-10},
	journal = {Revista Mexicana de Fitopatologia, Mexican Journal of Phytopathology},
	author = {López-Bautista, Viridiana and Mora-Aguilera, Gustavo and Gutiérrez-Espinosa, María Alejandra and Mendoza-Ramos, Coral and Martínez-Bustamante, Verónica Inés and Coria-Contreras, Juan José and Acevedo-Sánchez, Gerardo and Santana-Peñaloza, Baldemar},
	month = dec,
	year = {2019},
}

@Article{louw_pathogenicity_2015,
  author   = {Louw, Johannes Petrus and Korsten, Lise},
  journal  = {Plant Disease},
  title    = {Pathogenicity and {Host} {Susceptibility} of \textit{{Penicillium}} spp. on {Citrus}},
  year     = {2015},
  issn     = {0191-2917, 1943-7692},
  month    = jan,
  number   = {1},
  pages    = {21--30},
  volume   = {99},
  abstract = {Citrus fruit are exposed to numerous postharvest pathogens throughout the fresh produce supply chain. Well-known postharvest citrus fruit pathogens are Penicillium digitatum and P. italicum. Lesser-known pathogens include P. crustosum and P. expansum. This study examined pathogenicity and aggressiveness of Penicillium spp. present in fresh fruit supply chains on various Citrus spp. and cultivars. The impact of different inoculation methods and storage conditions on decay were also assessed. P. digitatum and P. italicum were the most aggressive Penicillium spp. on citrus but aggressiveness varied significantly over the evaluated citrus range. Decay and tissue-response lesions caused by P. crustosum were observed on ‘Nules Clementine’, ‘Nova’, ‘Owari Satsuma’, ‘Delta Valencia’, ‘Cambria Navel’, ‘Eureka’ seeded, and ‘Star Ruby’ for the first time. Likewise, these lesions caused by P. expansum were noted on Nules Clementine, Owari Satsuma, Delta Valencia, ‘Midknight Valencia’, and Eureka seeded for the first time. Tissue-response lesions affect fruit quality and some Penicillium spp. sporulated from the lesions, causing the inoculated species to complete their life cycle. New citrus–Penicillium spp. interactions were observed and the importance of monitoring inoculum loads of pathogens and nonhost pathogens were highlighted.},
  doi      = {10.1094/PDIS-02-14-0122-RE},
  language = {en},
  urldate  = {2022-02-10},
}

