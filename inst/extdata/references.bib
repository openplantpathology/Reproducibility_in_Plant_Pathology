@article{noriega-cantu_relationship_2019,
	title = {Relationship between spore fluctuations, environmental conditions and severity of calyx spot on roselle ({Hibiscus} sabdariffa)},
	volume = {38},
	issn = {2007-8080},
	url = {http://rmf.smf.org.mx/ojs/index.php/RMF/article/view/189},
	doi = {10.18781/R.MEX.FIT.1909-1},
	abstract = {La jamaica ({\textless}em{\textgreater}Hibiscus{\textless}/em{\textgreater} {\textless}em{\textgreater}sabdariffa{\textless}/em{\textgreater}) es afectada por severas enfermedades como tizón y manchado del cáliz (MCJ). El objetivo fue estudiar las relaciones entre fluctuación estacional de esporas de {\textless}em{\textgreater}Corynespora{\textless}/em{\textgreater} y {\textless}em{\textgreater}Coniella{\textless}/em{\textgreater} patógenos del manchado y tizón de cálices y hojas, las condiciones de clima y severidad (MCJ) en Ayutla, Gro., México. Las esporas fueron monitoreadas en dos ciclos, se aislaron los hongos y se realizó la identificación morfológica y molecular de {\textless}em{\textgreater}Coniella{\textless}/em{\textgreater} el estudio se centró en patógenos reportados en México. Las pruebas de patogenicidad con {\textless}em{\textgreater}C. diplodiella{\textless}/em{\textgreater} y {\textless}em{\textgreater}C. cassiicola{\textless}/em{\textgreater} confirmaron la sintomatología muy similar entre ambos hongos. {\textless}em{\textgreater}Coniella{\textless}/em{\textgreater} fue el más abundante (48\%) y con menos cantidad {\textless}em{\textgreater}Corynespora{\textless}/em{\textgreater} (8.7\%). La dinámica de esporas tuvo una fluctuación mensual y relacionada con el clima. Las temperaturas máximas y medias (°C) y promedio de radiación global (W/m2) mostraron correlaciones positivas significativas con la abundancia de esporas de {\textless}em{\textgreater}Coniella{\textless}/em{\textgreater} y el total de esporas, mientras que la precipitación (mm), temperatura mínima, velocidad del viento máxima (Km h-1) y horas de radiación solar, sus relaciones fueron negativas con las fluctuaciones de esporas de {\textless}em{\textgreater}Corynespora{\textless}/em{\textgreater}. La severidad del manchado de hojas y cálices fue correlacionada positiva y significativa con el número de esporas capturadas.},
	number = {1},
	urldate = {2022-02-10},
	journal = {Revista Mexicana de Fitopatología, Mexican Journal of Phytopathology},
	author = {Noriega-Cantú, David H. and Toledo-Aguilar, Rocío and Vasquez-Ortiz, Romualdo and Alejo-Jaimes, Antonino and Garrido-Ramírez, Eduardo R. and Pereyda-Hernández, Juan and González-Mateos, Ricardo},
	month = nov,
	year = {2019},
}

