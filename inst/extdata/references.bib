  journal  = {Phytopathologia Mediterranea},
  title    = {First report of blast of durum wheat in {Bangladesh}, caused by {Magnaporthe} oryzae pathotype {Triticum}},
  year     = {2021},
  issn     = {1593-2095, 0031-9465},
