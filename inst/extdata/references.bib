@Article{santosa_characterization_2021,
  author   = {Santosa, Adyatma I. and Ertunc, Filiz},
  journal  = {Phytopathologia Mediterranea},
  title    = {Characterization of two {Cucumber} mosaic virus isolates infecting {Allium} cepa in {Turkey}},
