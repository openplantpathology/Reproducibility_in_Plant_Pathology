@Article{carlucci_occurrence_2017,
  author   = {CARLUCCI, Antonia and LOPS, Francesco and MOSTERT, Lizel and HALLEEN, Francois and RAIMONDO, Maria Luisa},
  journal  = {Phytopathologia Mediterranea},
  title    = {Occurrence fungi causing black foot on young grapevines and nursery rootstock plants in {Italy}},
  year     = {2017},
  issn     = {15932095, 00319465},
  month    = feb,
  number   = {1},
  volume   = {56},
  abstract = {{\textless}p class="p1"{\textgreater}Summary.{\textless}strong{\textgreater} {\textless}/strong{\textgreater}Young grapevine plants with decline and wood necrosis symptoms were collected from vineyards and nurseries in the Apulia and Molise regions, Italy, from 2013 to 2015. Isolations of fungi were prepared from 45 diseased grapevine plants, and the cultures were identified. Several species commonly associated with Petri disease, Botryosphaeria dieback, and black foot disease were isolated. A detailed study was carried out, and 182 isolates resembling {\textless}em{\textgreater}Cylindrocarpon{\textless}/em{\textgreater}-like asexual forms were identified through morphological characterisation and DNA analysis of internal transcribed spacer regions 1 and 2 of the rRNA gene and the partial β-tubulin gene. {\textless}em{\textgreater}Dactylonectria torrensensis{\textless}/em{\textgreater} and {\textless}em{\textgreater}Ilyonectria liriodendri{\textless}/em{\textgreater} were identified based on morphological features and the partial histone 3 gene, so these fungi can be defined as the causal agents of black foot on grapevine for the first time in Italy. {\textless}em{\textgreater}Thelonectria blackeriella{\textless}/em{\textgreater} is also described as a new species, through morphological characterisation and multigenic analysis using sequence data for five loci (large subunit RNA, internal transcribed spacers, β-tubulin, actin, RNA polymerase II subunit 1). This new species was associated with black foot symptoms according to preliminary pathogenicity tests, with representative isolates of each of the three species. Pathogenicity tests showed that these species can cause black streaking in the wood of 1-year-old grapevine rootstock shoots. The identification of {\textless}em{\textgreater}D. torresensis{\textless}/em{\textgreater}, {\textless}em{\textgreater}I. liriodendri{\textless}/em{\textgreater} and {\textless}em{\textgreater}T. blackeriella{\textless}/em{\textgreater} from young grapevine plants and rooted rootstock highlights the importance of black foot disease in Italy, which has previously been overlooked.{\textless}/p{\textgreater}{\textless}br /{\textgreater}{\textless}em{\textgreater}{\textless}/em{\textgreater}},
  doi      = {10.14601/Phytopathol_Mediterr-18769},
  language = {eng},
  urldate  = {2022-02-10},
}

