@Article{Haidukowski2017,
  author   = {Haidukowski, Miriam and Cozzi, Giuseppe and Dipierro, Nunzio and Bavaro, Simona L. and Logrieco, Antonio F. and Paciolla, Costantino},
  journal  = {Phytopathologia Mediterranea},
  title    = {Decontamination of {Fumonisin} {B1} in maize grain by {\textless}em{\textgreater}{Pleurotus} eryngii{\textless}/em{\textgreater} and antioxidant enzymes},
  year     = {2017},
  issn     = {15932095, 00319465},
  month    = may,
  number   = {1},
  volume   = {56},
  abstract = {{\textless}p{\textgreater}Fumonisin B$_{\textrm{1}}$ (FB$_{\textrm{1}}$) is among the most common mycotoxins found in maize kernels and maize products worldwide. The microbiological process of detoxification and transformation of toxic organic pollutants is a promising method for foodstuffs decontamination. Some basidiomycetes, such as the {\textless}em{\textgreater}Pleurotus eryngii{\textless}/em{\textgreater} species complex, include several important commercial edible varieties that can detoxify polycyclic organic compounds and a range of wastes and pollutants. We investigated the potential role of {\textless}em{\textgreater}P. eryngii{\textless}/em{\textgreater}, one of the most consumed mushrooms, in the decontamination of FB$_{\textrm{1}}$ in maize. In addition, selected antioxidant enzymes, (soluble peroxidase (POD), catalase (CAT) and ascorbate peroxidase), primarily involved in control of cell hydrogen peroxide levels, and lignin degradation, were analyzed, to evaluate their contributions to the molecular mechanisms of FB$_{\textrm{1}}$ by {\textless}em{\textgreater}P. eryngii{\textless}/em{\textgreater}. FB$_{\textrm{1}}$ decontamination by {\textless}em{\textgreater}P. eryngii{\textless}/em{\textgreater} and involvement of CAT and POD enzymes in the control of toxic decontamination levels of H$_{\textrm{2}}$O$_{\textrm{2}}$ were demonstrated. A consistent reduction of FB$_{\textrm{1}}$ was observed at different incubation times. The average decrease levels of FB$_{\textrm{1}}$, with respect to the control cultures, ranged from 45 to 61\% (RSD \&lt; 15\%). This study is a possible eco-friendly approach to reducing this mycotoxin in the feed supply chains.{\textless}/p{\textgreater}},
  doi      = {10.14601/Phytopathol_Mediterr-20358},
  language = {eng},
  urldate  = {2022-02-10},
}

