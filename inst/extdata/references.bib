@Article{ortega-acosta_morphological_2019,
  author   = {Ortega-Acosta, Santo Ángel and Ochoa-Martínez, Daniel Leobardo and Hernández-Morales, Javier and Palemón-Alberto, Francisco},
  journal  = {Revista Mexicana de Fitopatología, Mexican Journal of Phytopathology},
  title    = {Morphological and genetic characterization of {Corynespora} cassiicola isolates obtained from roselle and associated weeds},
  year     = {2019},
  issn     = {2007-8080},
  month    = dec,
  number   = {1},
  volume   = {38},
  abstract = {Se analizaron morfológica y filogenéticamente diferentes aislados de {\textless}em{\textgreater}Corynespora cassiicola{\textless}/em{\textgreater} obtenidos de jamaica ({\textless}em{\textgreater}Hibiscus sabdariffa{\textless}/em{\textgreater}) y nueve malezas asociadas al cultivo. Los aislados fúngicos se dividieron en dos grupos: el grupo uno incluyó 37 aislados obtenidos de {\textless}em{\textgreater}H. sabdariffa{\textless}/em{\textgreater} y ocho obtenidos de otros hospedantes como {\textless}em{\textgreater}Solanum lycopersicum, Chromolaena odorata, Senna alata, Eugenia oerstediana, Passiflora viridiflora, Momordica charantia, Ricinus communis{\textless}/em{\textgreater} y {\textless}em{\textgreater}Gossypium hirsutum{\textless}/em{\textgreater}; en este caso, las colonias predominantes fueron de color gris, textura densa y forma poligonal. El grupo dos, incluyó a dos aislados obtenidos de{\textless}em{\textgreater} H. sabdariffa{\textless}/em{\textgreater} y uno de {\textless}em{\textgreater}Hyptis suaveolens{\textless}/em{\textgreater}; en este grupo, las colonias fueron principalmente de color crema y café pálido, densas y redondeadas. El análisis comparativo con aislados de {\textless}em{\textgreater}C. cassiicola{\textless}/em{\textgreater} de otros países mostró que los aislados del grupo uno referidos anteriormente se asociaron principalmente con aquellos aislados de la familia Solanaceae, mientras que los del grupo dos se relacionaron con los de la familia Cucurbitaceae. Para nuestro conocimiento este es el primer estudio sobre la caracterización morfológica y genética de {\textless}em{\textgreater}C. cassiicola{\textless}/em{\textgreater} en México.},
  doi      = {10.18781/R.MEX.FIT.1909-2},
  file     = {Full Text:/Users/adamsparks/Zotero/storage/MWN8ILXE/ortega-acosta_morphological_2019 - Morphological and Genetic Characterization of Corynespora Cassiicola Isolates Obtained from Roselle and Associated Weeds.pdf:application/pdf},
  url      = {http://rmf.smf.org.mx/ojs/index.php/RMF/article/view/194},
  urldate  = {2022-02-10},
}

